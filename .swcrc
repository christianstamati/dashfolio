{
	"jsc": {
		"target": "es2020",
		"parser": {
			"syntax": "typescript",
			"tsx": true,
			"decorators": true
		},
		"transform": {
			"react": {
				"runtime": "automatic"
			}
		},
		"loose": false,
		"externalHelpers": false,
		"keepClassNames": false
	},
	"module": {
		"type": "es6",
		"strict": false,
		"strictMode": true,
		"lazy": false,
		"noInterop": false
	},
	"minify": true,
	"env": {
		"targets": "> 0.5%, last 2 versions, not dead, not ie 11"
	}
}
